{
  "name": "cps-global-nav",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "test.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/cps-global-nav/cps-global-nav.esm.js",
  "exports": {
    ".": {
      "import": "./dist/cps-global-nav/cps-global-nav.esm.js",
      "require": "./dist/cps-global-nav/cps-global-nav.cjs.js"
    },
    "./cps-global-nav": {
      "import": "./dist/components/cps-global-nav.js",
      "types": "./dist/components/cps-global-nav.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CPS-Innovation/Polaris.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "deployed": "http-server ./www-deployed",
    "publish": "./publish.sh"
  },
  "devDependencies": {
    "@stencil/core": "^4.7.0",
    "@stencil/sass": "^3.0.12",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0"
  },
  "license": "MIT",
  "dependencies": {
    "@azure/msal-browser": "^2.38.1",
    "fetch-jsonp": "^1.3.0",
    "govuk-frontend": "^5.7.1",
    "http-server": "^14.1.1",
    "uuid": "^11.0.4"
  }
}
