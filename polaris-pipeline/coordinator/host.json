{
  "version": "2.0",
  "logging": {
    "fileLoggingMode": "debugOnly",
    "logLevel": {
      "default": "Warning",
      "Function": "Information",
      "coordinator.Durable.Orchestration.RefreshCaseOrchestrator": "Information",
      "coordinator.Functions.Maintenance.SlidingCaseClearDown": "Information",
      "coordinator.Services.OcrService.OcrService": "Information",
      "coordinator.Durable.Activity.ExtractText": "Information",
      "Host.Aggregator": "Information",
      "Host.Results": "Information",
      "Host.Triggers.DurableTask": "Information",
      "DurableTask.AzureStorage": "Warning",
      "DurableTask.Core": "Information",
      "Microsoft.Azure.WebJobs.Host.Scale": "None"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 20,
        "excludedTypes": "Request;Exception;Event;Trace"
      },
      "enableLiveMetrics": true,
      "enableDependencyTracking": true,
      "enablePerformanceCountersCollection": true,
      "httpAutoCollectionOptions": {
        "enableHttpTriggerExtendedInfoCollection": true,
        "enableW3CDistributedTracing": true,
        "enableResponseHeaderInjection": true
      }
    }
  },
  "extensions": {
    "durableTask": {
      "hubName": "%CoordinatorTaskHub%",
      "maxConcurrentActivityFunctions": 350,
      "maxConcurrentOrchestratorFunctions": 350,
      "tracing": {
        "traceInputsAndOutputs": false,
        "traceReplayEvents": false,
        "distributedTracingEnabled": true,
        "Version": "V2"
      }
    }
  }
}
